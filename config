Sources[$package_counter,DLFile]="v${HEADERS_MORE_VERSION}.tar.gz"
Sources[$package_counter,DLUrl]="https://github.com/openresty/headers-more-nginx-module/archive/refs/tags/v${HEADERS_MORE_VERSION}.tar.gz"
